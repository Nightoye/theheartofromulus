{"id":1,"name":"EV001","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":122,"indent":0,"parameters":[16,16,0,0,1]},{"code":211,"indent":0,"parameters":[0]},{"code":241,"indent":0,"parameters":[{"name":"2000_Peace","volume":100,"pitch":100,"pan":0}]},{"code":231,"indent":0,"parameters":[1,"Pic_2",0,0,0,0,100,100,255,0]},{"code":117,"indent":0,"parameters":[1]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\m[sys]那是许多年前发生的故事。"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\m[sys]有一位传说中的英雄，"]},{"code":401,"indent":0,"parameters":["征服了这片大陆。"]},{"code":401,"indent":0,"parameters":["他的名字叫做“罗慕路斯”。"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\m[sys]作为皇帝，他建立了前所未有的大帝国，"]},{"code":401,"indent":0,"parameters":["一手将人类带入繁荣。"]},{"code":401,"indent":0,"parameters":["文字，法律，城市……皆仰赖他的力量而建立。"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\m[sys]他与世界树的契约，"]},{"code":401,"indent":0,"parameters":["为他带来无尽的寿命，"]},{"code":401,"indent":0,"parameters":["将会成为永远君临这片土地的主宰。"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\m[sys]然而，很快，皇帝便堕落为暴君……"]},{"code":401,"indent":0,"parameters":["他开始怀疑身边的人会背叛自己，"]},{"code":401,"indent":0,"parameters":["也开始不再信赖他的臣民。"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\m[sys]无数人惨死在他的手上，"]},{"code":401,"indent":0,"parameters":["因为他无尽的力量、欲望与野心。"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\m[sys]忍受不了的人们终于奋起反抗，"]},{"code":401,"indent":0,"parameters":["疯狂的皇帝最终自取灭亡，"]},{"code":401,"indent":0,"parameters":["但帝国的宝物却流传到四方。"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\m[sys]然而，帝国的宝物再也没有昔日的力量，"]},{"code":401,"indent":0,"parameters":["只因为缺少一样关键。"]},{"code":401,"indent":0,"parameters":["\\! ——罗慕路斯的心脏。"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\m[sys]迄今为止，没有人见过它，"]},{"code":401,"indent":0,"parameters":["也没有人知道这件宝物到底是什么。"]},{"code":401,"indent":0,"parameters":["甚至有人怀疑，这东西只不过是个传说而已。"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\m[sys]直到，一个普通的雇佣兵，"]},{"code":401,"indent":0,"parameters":["和他的伙伴们的冒险开始……"]},{"code":242,"indent":0,"parameters":[3]},{"code":232,"indent":0,"parameters":[1,0,0,0,0,0,100,100,0,0,180,true]},{"code":235,"indent":0,"parameters":[1]},{"code":201,"indent":0,"parameters":[0,5,12,10,8,0]},{"code":123,"indent":0,"parameters":["A",0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true},{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":true,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true}],"x":0,"y":0}
